# Database
DATABASE_URL="postgresql://jacode:jacode@localhost:5432/jacode?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# MinIO (S3 Compatible Storage)
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=jacode-files

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# AI Model Configuration
AI_PROVIDER=ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=codellama:13b

# vLLM Configuration (Alternative)
VLLM_BASE_URL=http://localhost:8000
VLLM_MODEL=codellama/CodeLlama-13b-Instruct-hf

# Application
NODE_ENV=development
PORT=3000
BACKEND_PORT=4000
FRONTEND_URL=http://localhost:3000

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# ============================================================================
# Keycloak SSO (오프라인망 전용)
# ============================================================================

# Keycloak 서버 URL (예: http://keycloak.internal:8080)
KEYCLOAK_SERVER_URL=

# Keycloak Realm 이름
KEYCLOAK_REALM=jacode

# Keycloak Client ID
KEYCLOAK_CLIENT_ID=jacode-client

# Keycloak Client Secret
KEYCLOAK_CLIENT_SECRET=

# ============================================================================
# ChromaDB (Optional - Knowledge Base)
# ============================================================================
CHROMA_HOST=localhost
CHROMA_PORT=8000
